all:
  children:
    centos:
      hosts:
%{ for index, i in address_centos  ~}
        ${element(names_centos, index)}:
          ansible_host: ${element(address_centos, index)}
%{ endfor ~}
      vars:
        ansible_user: cloud-user
  vars:
    ansible_ssh_extra_args: '-o StrictHostKeyChecking=no'